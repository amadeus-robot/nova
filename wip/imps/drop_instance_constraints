<implementation>
 <name>drop_instance_constraints</name>
 <code>
drop_instance_constraints :: Array Token -> Array Token
drop_instance_constraints tokens =
  let
    Tuple _ after = elixirEnumSplitWhile (\t -> not (t.ttype == TTOperator && t.value == "<=")) tokens
  in
  case after of
    [] -> tokens
    h : rest ->
      if h.ttype == TTOperator && h.value == "<=" then
        rest
      else
        tokens
 </code>
 <tests>
  <test>
drop_instance_constraints [] == []
  </test>
  <test>
drop_instance_constraints [{ttype: TTIdentifier, value: "Show", line: 1, column: 1, pos: 1}, {ttype: TTOperator, value: "<=", line: 1, column: 6, pos: 6}, {ttype: TTIdentifier, value: "Int", line: 1, column: 9, pos: 9}] == [{ttype: TTIdentifier, value: "Int", line: 1, column: 9, pos: 9}]
  </test>
  <test>
drop_instance_constraints [{ttype: TTIdentifier, value: "Show", line: 1, column: 1, pos: 1}, {ttype: TTOperator, value: "=>", line: 1, column: 6, pos: 6}, {ttype: TTIdentifier, value: "Int", line: 1, column: 9, pos: 9}] == [{ttype: TTIdentifier, value: "Show", line: 1, column: 1, pos: 1}, {ttype: TTOperator, value: "=>", line: 1, column: 6, pos: 6}, {ttype: TTIdentifier, value: "Int", line: 1, column: 9, pos: 9}]
  </test>
  <test>
drop_instance_constraints [{ttype: TTIdentifier, value: "Show", line: 1, column: 1, pos: 1}, {ttype: TTDelimiter, value: ",", line: 1, column: 5, pos: 5}, {ttype: TTIdentifier, value: "Eq", line: 1, column: 7, pos: 7}, {ttype: TTOperator, value: "<=", line: 1, column: 10, pos: 10}, {ttype: TTIdentifier, value: "Int", line: 1, column: 13, pos: 13}] == [{ttype: TTIdentifier, value: "Int", line: 1, column: 13, pos: 13}]
  </test>
  <test>
drop_instance_constraints [{ttype: TTIdentifier, value: "Show", line: 1, column: 1, pos: 1}, {ttype: TTDelimiter, value: ",", line: 1, column: 5, pos: 5}, {ttype: TTIdentifier, value: "Eq", line: 1, column: 7, pos: 7}] == [{ttype: TTIdentifier, value: "Show", line: 1, column: 1, pos: 1}, {ttype: TTDelimiter, value: ",", line: 1, column: 5, pos: 5}, {ttype: TTIdentifier, value: "Eq", line: 1, column: 7, pos: 7}]
  </test>
 </tests>
</implementation>
